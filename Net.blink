option ClientOutput = "net/client.luau"
option ServerOutput = "net/server.luau"

event UpdateTransform {
    From: Server,
    Type: Unreliable,
    Call: SingleSync,
    Poll: true,
    Data: (f16, CFrame)
}

event SpawnMob {
    From: Server,
    Type: Reliable,
    Call: SingleSync,
    Poll: true,
    Data: (f16, CFrame, u8)
}
